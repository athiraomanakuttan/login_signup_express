<!DOCTYPE html>
<html lang="en">
  <%- include('header') %>
  <body>
    <div class="container-fluid main ">
        <div class=" col-md-4 signup-box  ">
            <h2 class="text-center">Sign Up</h2>
            <form>
                <div class="mb-3">
                  <input type="text" class="form-control" name="firstName"  placeholder=" First Name">
                </div>
                <div class="mb-3">
                  <input type="text" class="form-control" name="lastName"  placeholder="Last Name">
                </div>
                <div class="mb-3">
                    <input type="email" class="form-control" name="lastName"  placeholder="Email id">
                  </div>
                  <div class="mb-3">
                    <input type="text" class="form-control" name="lastName"  placeholder="User Name">
                  </div>
                  <div class="mb-3">
                    <input type="password" class="form-control" name="lastName"  placeholder="Password">
                  </div>
                <button type="submit" class="btn login-btn">Sign Up</button>
                <div class="mt-3 text-center">
                    <p for="exampleInputPassword1 " class="form-label">Already a Member ? <a href="/login">Login</a></p>
                  </div>
              </form>
        </div>
    </div>

    <%- include('footer') %>
  </body>
</html>
